{
    "name": "codica-studio/responsecache",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/responsecache",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/cache": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/support": "^8.0",
        "nesbot/carbon": "^2.35"
    },
    "require-dev": {
        "laravel/framework": "^8.0",
        "phpunit/phpunit" : "^9.1.5",
        "orchestra/testbench": "^6.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ResponseCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ResponseCache\\Test\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ResponseCache\\ResponseCacheServiceProvider"
            ],
            "aliases": {
                "ResponseCache": "Spatie\\ResponseCache\\Facades\\ResponseCache"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
